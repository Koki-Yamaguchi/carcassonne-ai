{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/:path*",
        "destination": "https://top-carcassonner.com/:path",
        "type": 301
      },
      {
        "source": "/",
        "destination": "https://top-carcassonner.com/",
        "type": 301
      }
    ]
  }
}
